<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ppdl.rxjava">

    <application
        android:name=".application.MiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rx.SecondActivity" />
        <activity android:name=".rx.ThreeActivity" />
        <activity android:name=".rx.FourActivity" />
        <activity android:name=".rx.FiveActivity" />
        <activity android:name=".rx.RxBusActivity" />
        <activity android:name=".rx2.RxJava2Activity"></activity>
    </application>

</manifest>