<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wx.review">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RetrofitActivity" />
        <activity android:name=".activity.KotlinActivity" />
        <activity android:name=".activity.AnnotationActivity" />
        <activity
            android:name=".activity.CycleLiftActivity"
            android:configChanges="keyboardHidden|orientation" />

        <service
            android:name=".service.TestService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".activity.ServiceActivity" />
        <activity android:name=".mvp.LoginActivity" />
        <activity android:name=".rxjava2.RxjavaActivity"></activity>
    </application>

</manifest>